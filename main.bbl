    \field{title}{{The Effect of Narrow Field of View and Information Density
  on Visual Search Performance in Augmented Reality}}
    \field{journaltitle}{2019 IEEE Conference on Virtual Reality and 3D User
  Interfaces (VR)}
    \field{year}{2019}
  \endentry

