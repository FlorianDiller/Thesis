    \field{title}{{Does Self-Modeling Affect Imagery Ability or Vividness?}}
    \field{volume}{4}
    \field{journaltitle}{Journal of Imagery Research in Sport and Physical
  Activity}
    \field{year}{2009}
  \endentry

