    \field{title}{{The impact of latency on perceptual judgments and motor
  performance in closed-loop interaction in virtual reality}}
    \field{journaltitle}{Proceedings of the 22nd ACM Conference on Virtual
  Reality Software and Technology}
    \field{year}{2016}
  \endentry

