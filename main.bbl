    \field{title}{{Joint angles tracking for rehabilitation at home using
  inertial sensors}}
    \field{journaltitle}{Proceedings of the 11th EAI International Conference
  on Pervasive Computing Technologies for Healthcare}
    \field{year}{2017}
  \endentry

