    \verb 10.3414/me13-01-0109
    \endverb
    \field{issn}{0026-1270}
    \field{number}{02}
    \field{pages}{145\bibrangedash 155}
    \field{title}{{Exercise Recognition for Kinect-based Telerehabilitation*}}
    \field{volume}{54}
    \field{journaltitle}{Methods of Information in Medicine}
    \field{year}{2015}
  \endentry

